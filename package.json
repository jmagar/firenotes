{
  "name": "@jmagar/axon",
  "version": "1.1.1",
  "description": "Self-hosted RAG pipeline for web scraping, semantic search, and AI-powered Q&A",
  "main": "dist/index.js",
  "bin": {
    "axon": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "local": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --write .",
    "type-check": "tsc --noEmit",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:unit": "vitest run --exclude **/*.integration.test.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.mjs",
    "test:e2e:strict": "AXON_E2E_STRICT_PREREQS=1 vitest run --config vitest.e2e.config.mjs",
    "test:e2e:watch": "vitest --config vitest.e2e.config.mjs",
    "test:all": "vitest run && pnpm test:e2e:strict",
    "test:all:lenient": "vitest run && pnpm test:e2e",
    "publish-beta": "npm publish --tag beta",
    "publish-prod": "npm publish --access public"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "keywords": [
    "axon",
    "rag",
    "cli",
    "web-scraping",
    "crawler",
    "scraper",
    "data-extraction",
    "llm",
    "markdown"
  ],
  "author": "jmagar",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmagar/axon.git"
  },
  "bugs": {
    "url": "https://github.com/jmagar/axon/issues"
  },
  "homepage": "https://github.com/jmagar/axon#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "husky": "^9.0.0",
    "lint-staged": "^16.2.7",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^4.12.1",
    "@types/proper-lockfile": "^4.1.4",
    "commander": "^14.0.3",
    "dotenv": "^17.2.3",
    "lru-cache": "^11.2.5",
    "p-limit": "^7.2.0",
    "proper-lockfile": "^4.1.2",
    "zod": "^4.3.6"
  }
}
